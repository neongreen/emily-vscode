{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "env": {
        "VSCODE_EXTENSION_ENABLEMENT": "true"
      },
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "Compile"
    }
  ]
}
